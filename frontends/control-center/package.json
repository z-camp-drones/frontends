{
  "name": "control-center",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "serve": "npm run http-server",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "serve-element": "vue-cli-service build --target wc --name control-center ./src/App.vue --watch",
    "serve-element:live": "npm run serve-element & npm run http-server",
    "build-element": "vue-cli-service build --target wc --name control-center ./src/App.vue",
    "http-server": "http-server -p 8081 ./dist"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "@types/socket.io-client": "^1.4.32",
    "core-js": "^2.6.5",
    "socket.io-client": "^2.2.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0"
  },
  "devDependencies": {
    "@types/vue": "^2.0.0",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-typescript": "^3.7.0",
    "@vue/cli-service": "^5.0.8",
    "@vue/web-component-wrapper": "^1.2.0",
    "http-server": "^0.11.1",
    "typescript": "^3.4.3",
    "vue-template-compiler": "^2.5.21"
  }
